<div class="container">
  <h1>Add a bookmark!</h1>

  <div class="container">
    <%= simple_form_for [@list, @bookmark] do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.submit "Bookmark this!" %>
    <% end %>
  </div>

  <%= link_to "Top", lists_path %>

</div>
